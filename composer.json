{
    "name": "digitaljunkies/test-bundle",
    "type": "library",
    "description": "Digital Junkies test bundle",
    "keywords": ["unit testing", "phpunit"],
    "homepage": "http://www.digital-junkies.uk",
    "license": "MIT",
    "authors": [
        {
            "name": "DJ Developers",
            "email": "developer@digital-junkies.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "symfony/form": "~2.1",
        "symfony/security-bundle": "~2.1",
        "phpunit/phpunit": "4.0.*",
        "fzaninotto/faker": "1.4.0"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "~2.1",
        "symfony/validator": "~2.1"
    },
    "autoload": {
        "psr-0": { "DJ\\TestBundle": "" }
    },
    "target-dir": "DJ/TestBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    }
}
